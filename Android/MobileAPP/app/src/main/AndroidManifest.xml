        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserInstructionsRASP"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name=".UserInstuctionsSTM"
            android:screenOrientation="portrait" />
